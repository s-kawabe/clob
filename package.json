{
  "name": "frourio-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm run migrate:dev && run-p dev:*",
    "dev:client": "next dev -p 8001",
    "dev:server": "npm run dev --prefix server",
    "dev:aspida": "aspida --watch",
    "dev:prisma": "cd ./server && npx prisma studio",
    "build": "run-p build:client build:server",
    "prebuild:client": "npm run generate",
    "build:client": "next build && next export",
    "build:server": "npm run build --prefix server",
    "generate": "run-p generate:*",
    "generate:aspida": "aspida",
    "generate:server": "npm run generate --prefix server",
    "lint": "eslint --ext .ts,.js,.tsx . && prettier --check \"./**/*.ts\"",
    "lint:fix": "eslint --ext .ts,.js,.tsx . --fix && prettier --write \"./**/*.ts\"",
    "migrate:dev": "npm run migrate:dev --prefix server",
    "migrate:dev:createonly": "npm run migrate:dev:createonly --prefix server",
    "migrate:reset": "npm run migrate:reset --prefix server",
    "start": "run-p start:*",
    "start:client": "next start",
    "start:server": "npm start --prefix server",
    "pretest": "npm run generate",
    "test": "jest",
    "pretypecheck": "npm run generate",
    "typecheck": "tsc --noEmit && tsc --noEmit -p server"
  },
  "dependencies": {
    "@aspida/axios": "^1.7.1",
    "@aspida/swr": "^1.7.1",
    "@auth0/auth0-react": "^1.8.0",
    "@chakra-ui/react": "^1.7.3",
    "@emotion/react": "^11",
    "@emotion/styled": "^11",
    "@prisma/client": "^3.6.0",
    "axios": "^0.21.1",
    "class-validator": "^0.13.1",
    "framer-motion": "^4",
    "frourio": "^0.25.1",
    "next": "^12.0.7",
    "prisma": "^2.21.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.3.1",
    "recoil": "^0.5.2",
    "swr": "^0.5.5"
  },
  "devDependencies": {
    "@testing-library/react": "^11.2.6",
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.41",
    "@types/react": "^17.0.3",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^5.6.0",
    "babel-jest": "^26.6.3",
    "cross-env": "^7.0.3",
    "dotenv": "^8.2.0",
    "eslint": "^7.24.0",
    "eslint-config-next": "^12.0.7",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "fastify": "^3.15.0",
    "fastify-cors": "^5.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "ts-jest": "^26.5.5",
    "ts-loader": "^8.1.0",
    "ts-node": "^9.1.1",
    "typescript": "~4.2"
  }
}
